{
    "name": "medphys/hadea",
    "description": "Project HADEA",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "batkor/editorjs_dependencies": "^1.0",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.3",
        "drupal/core": "^10.3",
        "drupal/core-composer-scaffold": "^10.3",
        "drupal/core-project-message": "^10.3",
        "drupal/core-recommended": "^10.3",
        "drupal/email_registration": "^2.0@RC",
        "drupal/entity_browser": "^2.9",
        "drupal/entity_browser_enhanced": "^2.0",
        "drupal/entity_reference_modal": "^1.0",
        "drupal/environment_indicator": "^4.0",
        "drupal/field_group": "^3.4",
        "drupal/gin": "^3.1",
        "drupal/gin_login": "^2.0",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/graphql": "^4.6",
        "drupal/honeypot": "^2.1",
        "drupal/httpswww": "^2.1",
        "drupal/key_auth": "^2.1",
        "drupal/mail_login": "^3.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/pathauto": "^1.11",
        "drupal/phpmailer_smtp": "^2.2",
        "drupal/realname": "^2.0@beta",
        "drupal/redirect": "^1.8",
        "drupal/restui": "^1.21",
        "drupal/role_based_theme_switcher": "^3.0",
        "drupal/samlauth": "^3.10",
        "drupal/smtp": "^1.4",
        "drupal/token": "^1.12",
        "drupal/turnstile": "^1.1",
        "drupal/twig_tweak": "^3.2",
        "drupal/views_migration": "^1.2",
        "drupal/webform": "^6.1",
        "drush/drush": "^13.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "extra": {
        "patches": {
            "drupal/entity_browser": {
                "3277056": "https://git.drupalcode.org/project/entity_browser/-/merge_requests/14.patch"
            },
            "drupal/role_based_theme_switcher": {
                "3480512":"https://www.drupal.org/files/issues/2024-10-14/3480512-Deprecated-function-Creation-of-dynamic-property--4.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^9 || ^10",
        "drupal/dev_mode": "^2.6",
        "drupal/devel": "^5.1",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.5"
    }
}
